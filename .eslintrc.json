{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "ignorePatterns": ["./../utils/functions.js"],
    "rules": {
        "linebreak-style": 0,
        "max-len": "off",
        "no-console": "off",
        "no-loop-func":"off",
        "import/extensions": "off",
        "no-param-reassign":"off",
        "no-plusplus":"off"
    }
}
